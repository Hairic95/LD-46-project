script/source = "extends Label

var time = 0.0
var notifications = []

func _ready():
	GLOBALS.NOTIFICATIONS = self

func _process(delta):
	if notifications.size() > 0 and time <= 0.0:
		text = notifications.pop_front()
		visible = true
		time = 2.0
	elif time <= 0.0:
		visible = false
	
	if time > 0.0:
		time -= delta

func notify(text):
	notifications.push_back(text)
"

